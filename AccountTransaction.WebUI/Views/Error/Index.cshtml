@{
    Layout = "_LayoutAccount";
}

<div class="container-xxl container-p-y">
    <div class="misc-wrapper">
        <h2 class="mb-2 mx-2">Página não encontrada :(</h2>
        <p class="mb-4 mx-2">Oops! 😖 A URL solicitada não foi encontrada neste servidor.</p>
        <a asp-controller="Home" asp-action="Index" class="btn btn-primary">Back to home</a>
        <div class="mt-3">
            <img src="~/theme/assets/img/illustrations/page-misc-error-light.png"
                 alt="page-misc-error-light"
                 width="500"
                 class="img-fluid"
                 data-app-dark-img="~/theme/assets/img/illustrations/page-misc-error-dark.png"
                 data-app-light-img="~/theme/assets/img/illustrations/page-misc-error-light.png" />
        </div>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}